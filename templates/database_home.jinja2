{% extends 'index.jinja2' %}
{% block title %}Databases{% endblock %}
{% block content %}
    <div class="float-left" style="padding-left: 45px; padding-top: 10px">
        <form method="post">
            <a class="btn btn-lg btn-success" href="{{ url_for('add_connect') }}">Add Connect</a>
        </form>
        <br>
        <div class="text-light">
            {% for i in db %}
                <a href="{{ url_for('connect_page', name_id = i[0]) }}" class="text-light btn"><h4>{{ i[0] }}</h4></a>
                <br>
            {% endfor %}
        </div>
    </div>
{% endblock %}
